{
  "name": "ts-agent-proxy",
  "version": "0.1.0",
  "description": "HTTP allowlist proxy for AI agent network filtering",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "proxy",
    "allowlist",
    "security",
    "ai-agent"
  ],
  "license": "MIT",
  "dependencies": {
    "node-forge": "^1.3.1",
    "picomatch": "^4.0.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "proxy-chain": "^2.5.4",
    "rate-limiter-flexible": "^5.0.4"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/node-forge": "^1.3.11",
    "@types/picomatch": "^3.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
